+        for chunk in chunks:
+            chunk_id = chunk.get('chunk_id', 'unknown')
+            metadata = chunk.get('metadata', {})
+            
+            score = 0.5  # Base score
+            factors = []
+            
+            doc_type = metadata.get('document_type', 'unknown')
+            if doc_type in ['10-K', '10-Q']:
+                score += 0.3
+                factors.append("SEC filing")
+            elif doc_type == '8-K':

+        elif agent_type == AgentType.KNOWLEDGE_MANAGER:
+            base_request["capability"] = "assess_knowledge_health"
+        
+        return base_request
+    
+    async def _synthesize_agent_results(self, results: Dict[str, Any], original_request: Dict[str, Any]) -> Dict[str, Any]:

+    def _classify_table_type(self, table) -> str:
+        """Classify the type of financial table"""
+        if hasattr(table, 'cells'):
+            table_text = ' '.join([cell.content for cell in table.cells]).upper()
+            
+            if "BALANCE SHEET" in table_text:
